<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax.ui">
		<metaInformationProvider class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsUIMetaInformation" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsUIMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="owl2fs" icon="icons/editor_icon.gif" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsEditor" name="EMFText owl2fs Editor">
		</editor>
	</extension>
	
	<extension id="west.twouse.language.owl2fs.resource.owl2fs.ui.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Owl2fs Text Editor" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsPreferencePage" class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsSyntaxColoringPreferencePage" class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsSyntaxColoringPreferencePage" category="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsPreferencePage">
		</page>
		<page name="Brackets" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsBracketPreferencePage" class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsBracketPreferencePage" category="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsPreferencePage">
		</page>
		<page name="Occurrence" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsOccurrencePreferencePage" class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsOccurrencePreferencePage" category="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsNewFileWizard" id="west.twouse.language.owl2fs.resource.owl2fs.ui.Owl2fsNewFileWizard" name="EMFText .owl2fs file">
		</wizard>
	</extension>
	
</plugin>
